package com.logic.map;

import java.util.List;

public class MapFindPath
{
    /** 是否可达 */
    public boolean reachable;
    /** 路径 */
    public List<PathANode> step;

    public MapFindPath(boolean reachable, List<PathANode> list)
    {
        this.reachable = reachable;
        step = list;
    }
}
